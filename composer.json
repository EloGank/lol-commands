{
   "name": "elogank/lol-replay-downloader-cli",
   "description": "League of Legend replay downloader without any software. Replays are saved into the cloud.",
   "keywords": ["lol", "command", "league of legends", "replay", "downloader", "record", "recorder", "cli", "console"],
   "licence": "MIT",
   "authors": [
       {
           "name": "Sylvain Lorinet",
           "email": "sylvain.lorinet@gmail.com"
       }
    ],
    "require": {
        "php": ">=5.4.0",
        "incenteev/composer-parameter-handler": "~2.1.0",
        "symfony/yaml": "~2.5.7",
        "symfony/console": "~2.5.5",
        "elogank/lol-replay-downloader": "1.0.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "config/config.yml",
            "parameter-key": "config"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "EloGank": "src/"
        }
    }
}
